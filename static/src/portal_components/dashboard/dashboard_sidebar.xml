                            <ul class="dashboard-mainmenu rbt-default-sidebar-list">
                                <li>
                                    <a href="#" data-action="settings" t-on-click="() => { handleTabClick('setting'); }">
                                        <i class="feather-settings"></i>
                                        <span>Settings</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="index.html" data-action="logout">
                                        <i class="feather-log-out"></i>
                                        <span>Logout</span>
                                    </a>
                                </li>
                            </ul>
