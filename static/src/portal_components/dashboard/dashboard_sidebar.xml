<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="enmasys_multiple_vendor.DashboardSidebar">
        <!-- Start Dashboard Sidebar  -->
        <div class="rbt-default-sidebar sticky-top rbt-shadow-box rbt-gradient-border">
            <div class="inner">
                <div class="content-item-content">
                    <div class="rbt-default-sidebar-wrapper">
                        <!-- Student Section -->
                        <div class="section-title mb--20">
                            <h6 class="rbt-title-style-2">Welcome, Jone Due</h6>
                        </div>
                        <nav class="mainmenu-nav">
                            <ul class="dashboard-mainmenu rbt-default-sidebar-list">
                                <li>
                                    <a href="#" data-action="dashboard" t-on-click="() => { handleTabClick('dashboard', 'instructor-dashboard.html'); }">
                                        <i class="feather-home"></i>
                                        <span>Dashboard</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" data-action="profile" t-on-click="() => { handleTabClick('profile', 'instructor-profile.html'); }">
                                        <i class="feather-user"></i>
                                        <span>My Profile</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" data-action="enrolled_courses" t-on-click="() => { handleTabClick('enrolled_courses', 'instructor-enrolled-courses.html'); }">
                                        <i class="feather-book-open"></i>
                                        <span>Enrolled Courses</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" data-action="wishlist" t-on-click="() => { handleTabClick('wishlist'); }">
                                        <i class="feather-bookmark"></i>
                                        <span>Wishlist</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" data-action="reviews" t-on-click="() => { handleTabClick('reviews'); }">
